{
  "name": "frontend-rdj",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install -w client && npm install -w server",
    "build": "npm run build -w client",
    "start": "node server/server.js",
    "dev:api": "npm run dev -w server",
    "dev:client": "npm run dev -w client",
    "dev": "npm run dev:api & npm run dev:client"
  },
  "engines": {
    "node": ">=18"
  }
}
